/* Requires the Docker Pipeline plugin */
pipeline {
    agent {label 'slave01'}
    stages {
        stage('build') {
            steps {
                sh 'docker ps'
                sh 'docker-compose version'
            }
          stage('test') {
            steps {
                sh 'docker ps'
                sh 'docker-compose version'
            }
            stage('deploy') {
            steps {
                sh 'docker ps'
                sh 'docker-compose version'
            }
            stage('E2E_test') {
            steps {
                sh 'docker ps'
                sh 'docker-compose version'
            }
        }
    }
}
